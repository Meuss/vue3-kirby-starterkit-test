{
  "root": true,
  "env": {
    "es2021": true,
    "browser": true,
    "node": true,
    "vue/setup-compiler-macros": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "prettier"],
  "parserOptions": {
