{
  "private": true,
  "version": "4.1.0",
  "scripts": {
    "kirby": "env-cmd --use-shell \"php -S \\$KIRBY_DEV_HOSTNAME:\\$KIRBY_DEV_PORT -t public vendor/getkirby/cms/router.php\"",
    "dev": "shx touch src/.lock && concurrently \"npm:kirby\" \"vite\"",
    "build": "shx rm -f src/.lock && vite build && node scripts/buildServiceWorker.mjs",
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,vue}\""
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.2.4",
    "concurrently": "^7.0.0",
    "consola": "^2.15.3",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.5.0",
    "nanoid": "^3.3.2",
    "prettier": "2.6.1",
    "shx": "^0.3.4",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.8.6"
  }
}
